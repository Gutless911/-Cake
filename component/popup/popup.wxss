.shadow{
  width: 100%;
  height: 100%;
  z-index: 80;
  position: absolute;
  top: 0;
  background-color: #000;
  opacity: 0.5;
}
/* 上滑高度 */
.choosen{
  width: 100%;
  height: 80%;
  position: fixed;
  bottom: 0;
  background-color: #fff;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  z-index: 98;
}
.ChoosenActive{
  height: auto !important;
  max-height: 80%  !important;
}
/* 内容 */
.container-box {
  display: flex;
  height: 100%;
  flex-direction: column;
  width: 100%;
  border-radius: 15rpx;
  margin: 0 auto;
  z-index: 98;
  padding: 0 20rpx;
  box-sizing: border-box;
  overflow: hidden;
  overflow-y: scroll;
  white-space:nowrap;
}
.box-title{
  display:flex;
  width: 100%;
  height: 140rpx;
  position: fixed;
  top: 20rpx;
  background-color: #fff;
  border-bottom: 1px solid #F7F7F7;
  padding-bottom: 20rpx;
}
.title-img{
  display:flex;
  width:30%;
}
.title-img image{
  width:100% !important;
  min-height: 120rpx !important;
  height: auto;
}
.title-name{
  box-sizing: border-box;
  display: flex;
  width: 80%;
  padding-left:20rpx;
}
.title-name .icon-cha{
  display: flex;
  height: 80rpx;
  width: 20%;
  color: #c8c9cc;
  font-size: 44rpx;
  align-items: center;
}
.title-text{
  width: 100%;
}
.title-text text:nth-child(1){
  font-size: 28rpx;
}
.title-text text:nth-child(2){
  display: flex;
  font-size: 34rpx;
  font-weight: bolder;
  color: #c3a769;
}
.box-detailed{
  display: flex;
  width: 100%;
  max-height: 467px;
  flex-direction: column;
  border-bottom: 1px  solid #F7F7F7;
  margin: 20rpx 0;
  margin-top: 195rpx;

}
.detailed-size{
  display: flex;
  width: 100%;
  flex-direction: column;
}
.detailed-size view:nth-child(1){
  display: flex;
  width: 100%;
  margin-bottom: 24rpx;
}
.detailed-size view:nth-child(2){
  display: flex;
  width: 85%;
  flex-wrap:wrap;
}
.detailed-size view:nth-child(2) text{
  display: flex;
  padding: 10rpx;
  border: 1px solid #999999;
  margin: 0 24rpx 24rpx 0;
  color: #323233;
  font-size: 26rpx;
}
.textActive{
  color: #c3a769 !important;
  background-color: #F9F6F0 !important;
  border: 1px solid #F9F6F0 !important;
}
.box-num{
  display: flex;
  width: 100%;
  margin-bottom: 120rpx;
}
.num-left{
  width: 50%;
  display: flex;
}
.num-right{
  width: 50%;
  display: flex;
  justify-content: flex-end;
}
.num-right .iconfont{
  display: flex;
  width: 56rpx;
  height: auto;
  background-color: #f2f3f5;
  margin: 0 5rpx;
  justify-content: center;
  align-items: center;
  color: #323233;
}
.sl{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 60rpx;
  height: auto;
  background-color: #f2f3f5;
  margin: 0 5rpx;
  color: #323233;
  font-size: 28rpx;

}
.box-purchase{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  display: flex;
}
.box-purchase view{
  display: flex;
  width: 50%;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
}
.box-purchase view:nth-child(1){
  background: #f3eee1;
  color: #c3a769;
}
.box-purchase view:nth-child(2){
  background: #c3a769;
  color: #fff;
}